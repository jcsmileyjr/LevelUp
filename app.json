{
  "expo": {
    "name": "Level Up",
    "slug": "LevelUp",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "description":"A mobile app that focuses on developers guiding their learning path by planning goals,  breaking those goals down into milestones, and displaying a timeline of successes.",
    "android":{
      "package":"com.levelup.mobile"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/small-hoodie.png",
    "splash": {
      "image": "./assets/levelup-hoodie.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
